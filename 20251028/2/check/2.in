a = Triangle((0, 0), (4, 0), (2, 4))
b = Triangle((1, 1), (5, 1), (3, 5))
print(f"a area: {abs(a):.3f}, b area: {abs(b):.3f}")
print(f"a < b: {a < b}, b < a: {b < a}")
print(f"a in b: {a in b}, b in a: {b in a}")
print(f"a & b: {a & b}")
c = Triangle((1, 1), (3, 1), (2, 2))
print(f"c in a: {c in a}")
print(f"c & a: {c & a}")